    <rules>
        <fullName>Webinar No Show</fullName>
        <actions>
            <name>Webinar_No_Show</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND(  NOT(ISBLANK(Training__r.Webinar_ID__c)),  ISPICKVAL(Training__r.Delivery_Method__c, &quot;Webinar&quot;),  IF(Citrix_Join__c&gt;=Training__r.Start_Date__c,(Citrix_Leave__c-Citrix_Join__c)*24*60,  IF(Training__r.Start_Date__c&gt;Citrix_Join__c,(Citrix_Leave__c-Training__r.Start_Date__c)*24*60,  null))/Training__r.Training_Duration__c&lt;0.5 )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
