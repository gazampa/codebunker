        <fullName>Was Placeholder</fullName>
        <actions>
            <name>Was_Placeholder</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>AND( ISCHANGED(Contact__c), OR(  PRIORVALUE(Contact__c) = $Label.Placeholder,  PRIORVALUE(Contact__c) = $Label.Placeholder_2,  PRIORVALUE(Contact__c) = $Label.Placeholder_3,  PRIORVALUE(Contact__c) = $Label.Placeholder_4,  PRIORVALUE(Contact__c) = $Label.Placeholder_5  ) )</formula>
