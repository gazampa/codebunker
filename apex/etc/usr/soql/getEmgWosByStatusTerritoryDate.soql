select id, workordernumber, SAF_Parent_Service_Territory_Name__c, locationid, caseid, recordtype.name, SAF_Order_Type_Name__c, Due_Date__c, Earliest_Start_Permitted__c, ( select id, appointmentnumber from ServiceAppointments ) from WorkOrder where status = 'New' and Priority='1 - Emergency' and Emergency__c = true and createddate > 2022-05-01T10:04:00.000+0000 and ( SAF_Parent_Service_Territory_Name__c = 'UNY-EAST' or SAF_Parent_Service_Territory_Name__c = 'RHODE ISLAND GAS' )limit 100