select id, appointmentnumber, parentrecordid, ParentRecordType, createddate, DueDate, EarliestStartTime, NG_Finish_No_Later_Than__c, SchedStartTime, SchedEndTime from ServiceAppointment where status = 'New' and createddate < 2022-05-15T10:04:00.000+0000 and createddate > 2022-05-01T10:04:00.000+0000